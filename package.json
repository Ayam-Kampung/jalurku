{
  "name": "jalurku",
  "version": "1.0.0",
  "description": "Website untuk JHIC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ayam-Kampung/jalurku.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i src/css/input.css -o static/css/output.css --minify",
    "dev:css": "npx @tailwindcss/cli -i src/css/input.css -o static/css/output.css --watch",
    "dev": "esbuild src/js/app.js --bundle --outfile=static/js/bundle.js --sourcemap --watch",
    "build": "esbuild src/js/app.js --bundle --outfile=static/js/bundle.js --minify"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "gsap": "^3.13.0",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "esbuild": "^0.25.10"
  }
}
